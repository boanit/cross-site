{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "CrossSite: Node",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
			"args": ["develop"],
			"stopOnEntry": false,
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": ["--nolazy"],
			"env": {
				"NODE_ENV": "development",
				"DEBUG": "gatsby:*"
			},
			"console": "externalTerminal",
			"sourceMaps": false,
			"outFiles": []
		},
		{
			"name": "CrossSite: Build",
			"type": "node",
			"request": "launch",
			"protocol": "inspector",
			"program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
			"args": ["build"],
			"stopOnEntry": false,
			"runtimeArgs": ["--nolazy"],
			"sourceMaps": false
		}
	]
}
